{
  "name": "Stamina",
  "version": "1.0",
  "permissions": ["activeTab", "notifications",
         "storage"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "description": "Take a break from your computer to increase your productivity!",
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "manifest_version": 2
}





